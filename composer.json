{
    "license": "proprietary",
    "name": "jrbeeman/watercooler",
    "description": "Watercooler Drupal project, built using acquia/blt.",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/audio_embed_field",
                "version": "8.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupal.org/sandbox/vilepickle/2784301.git",
                    "type": "git",
                    "reference": "refs/heads/8.x-1.x"
                }
            }
        }
    ],
    "require": {
        "acquia/blt": "^8.3",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "dev-master#5456199acdcc16b243814aa45259f4c56272a634 as 1.5.0",
        "drupal/acquia_connector": "^8.1.5",
        "drupal/acsf": "^8.1.32",
        "drupal/console": "1.0.0-beta5",
        "drupal/core": "~8",
        "drupal/lightning": "8.1.x-dev",
        "drupal/memcache": "^8.2.0-alpha2",
        "drupal/search_api": "8.1.0-alpha14",
        "drupal/search_api_solr": "8.1.0-alpha3",
        "drupal/security_review": "8.*",
        "drupal-composer/drupal-security-advisories": "8.0.x-dev",
        "wikimedia/composer-merge-plugin": "^1.3",
        "roave/security-advisories": "dev-master",
        "drupal/markdown": "8.1.*",
        "michelf/php-markdown": "1.x",
        "drupal/libraries": "8.x",
        "drupal/flag": "8.4.*",
        "drupal/recaptcha": "~8",
        "drupal/audio_embed_field": "^8.0",
        "drupal/cog": "^8.1.0",
        "drupal/features": "^8.3.0-beta9"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "~1.7",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "^1.3.1",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "drupal/coder": "^8.2.9",
        "drupal/drupal-extension": "^3.2",
        "drush/drush": "^9.0",
        "drupal-composer/drupal-scaffold": "^2.1.0",
        "jakoch/phantomjs-installer": "dev-master as 1.9.8",
        "jarnaiz/behat-junit-formatter": "^1.3.2",
        "phing/phing": "~2",
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.7",
        "drupal/devel": "~8",
        "geerlingguy/drupal-vm": "~3.1",
        "se/selenium-server-standalone": "^2.53"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Ignore front end vendor folders to improve directory search performance": "https://www.drupal.org/files/issues/ignore_front_end_vendor-2329453-116.patch"
            },
            "drupal/features": {
                "Bundle support": "https://www.drupal.org/files/issues/features-2808303-2.patch"
            }
        },
        "merge-plugin": {
            "require": [
                "docroot/modules/custom/decoda/composer.json"
            ]
        },
        "blt": {
            "update": true
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "./vendor/acquia/blt/blt.sh create-project"
        ],
        "install-phantomjs": [
            "rm -rf vendor/bin/phantomjs",
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "blt-alias": "blt install-alias -Dcreate_alias=true",
        "nuke": [
            "rm -rf vendor composer.lock",
            "@composer clearcache",
            "@composer install"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
